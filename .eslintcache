 .then( res => {\n        const arrayOfBeer = res.data\n        this.setState({ arrayOfBeer })\n      })    \n  }\n\n  moveId = (movedId) => {\n    this.setState({ arrayOfFavBeer: [...this.state.arrayOfFavBeer, movedId] })\n  }\n\n  removeId = (removedId) => {\n    const copyOfFavs = this.state.arrayOfFavBeer\n    copyOfFavs.map((beer, i) => {\n      beer === removedId && copyOfFavs.splice(i, 1)\n      this.setState({ arrayOfFavBeer: [...copyOfFavs]})\n    })\n  \n  }\n\n  render() {\n\n    return (\n      <div className=\"App\">\n          <header className=\"App-body Bar-bckgrnd\">\n          <img src={logo} className=\"App-logo\" alt=\"logo\" />\n          <h1 className=\"App-name\">üç∫üçªüç∫ faveBr√ºs üçªüç∫üçª</h1>\n        <div style={{display: \"flex\", flexDirection: \"column\", alignItems: \"center\", justifyContent: \"center\"}}>\n            \n            {this.state.arrayOfFavBeer.length === 0 ?\n            <AnchorLink href=\"#main\" className=\"App-tag\">‚¨áÔ∏è Like a beer below to add it to your faves ‚¨áÔ∏è</AnchorLink> :\n            <h3 className=\"App-tag\">My Faves</h3> }\n            <ul style={{ display: \"flex\", flexWrap: \"wrap\", justifyContent: \"center\", alignItems: \"center\"}}>\n\n            \n              {this.state.arrayOfBeer.map(beer => \n              <li style={{zIndex: 999, listStyle: \"none\"}}>\n                {this.state.arrayOfFavBeer.includes(beer.id) && \n                <FavCard  \n                key={beer.id} \n                id={beer.id} \n                name={beer.name}  \n                image_url={beer.image_url}\n                removeId={this.removeId}\n                />\n              \n              }\n              </li>\n              )}\n            </ul>\n        </div>\n                </header>\n            <body className=\"App-body\" id=\"main\">\n          <ol style={{zIndex: 999}}>{this.state.arrayOfBeer.map((beer) => {\n            return (\n              <BeerCard \n                key={beer.id}\n                id={beer.id}\n                name={beer.name} \n                image_url={beer.image_url}\n                first_brewed={beer.first_brewed}\n                tagline={beer.tagline}\n                abv={beer.abv}\n                description={beer.description}\n                moveId={this.moveId}\n                removeId={this.removeId}\n                fillHeart={this.fillHeart}\n                arrayOfBeer={this.state.arrayOfBeer}\n                arrayOfFavBeer={this.state.arrayOfFavBeer}\n              />\n            )\n          })}\n          </ol>\n          <ScrollUpButton className=\"Scroll-up\"/>\n        </body>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/reportWebVitals.js",[],"/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/BeerCard.js",["51","52","53"],"import React, { useState } from 'react'\nimport HeartButton from './HeartButton'\nimport UnHeartButton from './UnHeartButton'\n\nconst BeerCard = (props) => {\n    const {id, name, image_url, malts, hops, yeast} = props\n    const [isHidden, setHide] = useState(true)\n\n    return(\n        <div id={id} style={{ zIndex: 999, listStyle: \"none\", display: \"flex\", background: \"#333\", margin: \"20px\", borderRadius: \"10px\", padding: \"20px\", alignItems: \"center\", width: \"60vw\"}}>\n            <img \n                style={{ height: \"300px\", margin: \"20px\" }} \n                src={props.image_url}\n                alt=\"image of beer\"\n                onClick={()=> {isHidden ? setHide(false) : setHide(true)}}\n            ></img>\n            {isHidden ?\n                <div></div> :\n                <div>\n                    <p>Malts: {malts}</p>\n                    <p>Hops: {hops}</p>\n                    <p>Yeast: {yeast}</p>\n                </div>\n            }\n            <div style={{padding: \"20px\"}}>\n                <h5>First brewed: <span>{props.first_brewed}</span></h5>\n                <p>ABV: {props.abv}</p>\n            </div>\n            <div>\n                <h2>{props.name}</h2>\n                <h4>{props.tagline}</h4>\n                <p style={{fontSize: \"18px\"}}>{props.description}</p>                \n                <div>\n                    {props.arrayOfFavBeer.includes(props.id) ?\n                    <UnHeartButton id={props.id} removeId={props.removeId} /> :\n                    <HeartButton id={props.id} moveId={props.moveId} /> \n                    }\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default BeerCard;","/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/HeartButton.js",["54","55","56","57"],"import React, { Component, useState } from 'react';\n\nexport default function HeartButton(props) {\n\n    const [fav, setFav] = useState(false)\n\n\n    const handleClick = () => {\n        props.moveId(props.id)\n    }\n\n    const buttonStyle = {\n            background: \"transparent\",\n            border: \"none\",\n            cursor: \"pointer\",\n            color: \"tomato\",\n            outline: \"none\",\n            fontSize: \"30px\"\n        }\n\n        const openHeart = <i className=\"far fa-heart\"></i>\n        const filledHeart = <i className=\"fas fa-heart\"></i>\n    \n    return (\n        <button \n            style={buttonStyle} \n            onClick={handleClick}> \n            {openHeart}\n        </button>\n    )\n}","/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/Favs.js",[],["58","59"],"/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/FavCard.js",[],"/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/UnHeartButton.js",["60","61","62","63"],"import React, { Component, useState } from 'react'\n\nexport default function UnHeartButton(props) {\n\n    const [fav, setFav] = useState(true)\n\n\n    const handleClick = () => {\n        props.removeId(props.id)\n    }\n\n    const buttonStyle = {\n            background: \"transparent\",\n            border: \"none\",\n            cursor: \"pointer\",\n            color: \"tomato\",\n            outline: \"none\",\n            fontSize: \"30px\"\n        }\n        \n    const openHeart = <i className=\"far fa-heart\"></i>\n    const filledHeart = <i className=\"fas fa-heart\"></i>\n\n    return (\n        <button \n        style={buttonStyle} \n        onClick={handleClick}> \n        {filledHeart}\n        </button>\n    )\n}","/Users/lanegarner/Documents/Dev/aca-react/axios-react-tutorial/src/RemoveFav.js",["64","65","66"],"import React, { Component, useState } from 'react';\nimport ('./RemoveFav.css');\n\nexport default function RemoveFav(props) {\n    \n    const [fav, setFav] = useState(true)\n\n    const handleClick = () => {\n        props.removeId(props.id)\n    }\n\n    const buttonStyle = {\n            background: \"transparent\",\n            border: \"none\",\n            cursor: \"pointer\",\n            outline: \"none\",\n            fontSize: \"30px\",\n            position: \"absolute\",\n            transform: \"translate(-40px, -20px)\",\n            fontWeight: 100,\n        }\n        \n    return (\n        <button\n        className=\"Close-button\"\n        style={buttonStyle} \n        onClick={handleClick}> \n        x\n        </button>\n    )\n}",{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":26},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":33,"column":30,"nodeType":"77","messageId":"78","endLine":33,"endColumn":32},{"ruleId":"67","severity":1,"message":"79","line":6,"column":16,"nodeType":"69","messageId":"70","endLine":6,"endColumn":20},{"ruleId":"67","severity":1,"message":"80","line":6,"column":22,"nodeType":"69","messageId":"70","endLine":6,"endColumn":31},{"ruleId":"81","severity":1,"messag